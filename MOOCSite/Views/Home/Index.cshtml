@using MOOCSite.Models
@model IEnumerable<User>

@{
    ViewData["Title"] = "All Users";
}

<h2>All Reservations</h2>

<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>ID</th>
            <th>Login</th>
            <th>Password</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model)
        {
                <tr>
                    <td>@r.Id</td>
                    <td>@r.Login</td>
                    <td>@r.Password</td>
                    <td>@r.Email</td>
                </tr>
        }
    </tbody>
</table>